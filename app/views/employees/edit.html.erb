<h1>Update Employee Form</h1>
<h2> <%= @employee.full_name %> </h2><br>

<%= form_for @employee do |f| %>
    <%= f.label :title%>
    <%= f.text_field :title %><br>
    <%= f.label :office %>
    <%= f.text_field :office %><br>
    <%= f.label :img_url %>
    <%= f.text_field :img_url %><br>
    <%= f.collection_select :dog_id, @dogs, :id, :name %>
<%end %>