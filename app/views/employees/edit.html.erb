<h1> Employee Form </h1>

<%= form_for @employee do |f| %>



    <%= f.label :alias %>
    <%= f.text_field :alias %>

    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :office %>
    <%= f.text_field :office %>

    <%= f.label :img_url %>
    <%= f.text_field :img_url %>

    <%= f.label "Dog" %>
    <%= f.collection_select :dog_id, Dog.all, :id, :name %>

    <%= f.submit %>

<% end %>